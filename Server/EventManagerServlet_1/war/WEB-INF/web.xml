<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>EventManagerServlet_1</servlet-name>
		<servlet-class>com.main.EventManagerServlet_1</servlet-class>
	</servlet>
	
	 <servlet>
		<servlet-name>insertData</servlet-name>
		<servlet-class>com.support.InsertData</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>com.main.Login</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>EventManagerServlet_1</servlet-name>
		<url-pattern>/eventmanagerservlet_1</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>insertData</servlet-name>
		<url-pattern>/insertData</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>getGroupDescription</servlet-name>
		<servlet-class>com.main.GetGroupDescription</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>getGroupDescription</servlet-name>
		<url-pattern>/getGroupDescription</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
